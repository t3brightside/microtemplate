<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<ROOT type="array">
		<type>array</type>
		<el type="array">
			<alignment type="array">
				<TCEforms type="array">
					<label>Content alignment</label>
					<config type="array">
						<type>selectSingle</type>
						<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">Default</numIndex>
								<numIndex index="1">1</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">Center</numIndex>
								<numIndex index="1">2</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</alignment>
			<nopadding>
				<TCEforms>
					<label>No padding</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</nopadding>
			<fullwidth>
				<TCEforms>
					<label>Full page width content</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</fullwidth>
			<fullheight>
				<TCEforms>
					<label>100% of browser height</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</fullheight>
			<vcenter>
				<TCEforms>
					<label>Content vertically centred (works only with the setting above)</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</vcenter>
			<class type="array">
				<TCEforms type="array">
					<config type="array">
						<type>input</type>
						<size>48</size>
						<eval>trim</eval>
					</config>
					<label>Custom CSS classes</label>
				</TCEforms>
			</class>
			<textcolor type="array">
				<TCEforms type="array">
					<label>Text color</label>
					<config type="array">
						<type>selectSingle</type>
						<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">Default</numIndex>
								<numIndex index="1">0</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">Light</numIndex>
								<numIndex index="1">1</numIndex>
							</numIndex>
							<numIndex index="2" type="array">
								<numIndex index="0">Dark</numIndex>
								<numIndex index="1">2</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</textcolor>
			<bgcolordefined type="array">
				<TCEforms type="array">
					<label>Background color predefined</label>
					<config type="array">
						<type>selectSingle</type>
						<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">Default</numIndex>
								<numIndex index="1">0</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">Light</numIndex>
								<numIndex index="1">1</numIndex>
							</numIndex>
							<numIndex index="2" type="array">
								<numIndex index="0">Dark</numIndex>
								<numIndex index="1">2</numIndex>
							</numIndex>
							<numIndex index="3" type="array">
								<numIndex index="0">Black</numIndex>
								<numIndex index="1">3</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</bgcolordefined>
			<bgcolor type="array">
				<TCEforms type="array">
					<config type="array">
						<type>input</type>
						<size>48</size>
						<eval>trim</eval>
						<renderType>colorpicker</renderType>
					</config>
					<label>Background color (overrides setting above)</label>
				</TCEforms>
			</bgcolor>
			<image>
                    <label>Background mage</label>
                    <config>
                        <type>inline</type>
                        <foreign_table>sys_file_reference</foreign_table>
                        <foreign_field>uid_foreign</foreign_field>
                        <foreign_sortby>sorting_foreign</foreign_sortby>
                        <foreign_table_field>tablenames</foreign_table_field>
                        <foreign_match_fields>
                            <fieldname>image</fieldname>
                        </foreign_match_fields>
                        <foreign_label>uid_local</foreign_label>
                        <foreign_selector>uid_local</foreign_selector>
                        <overrideChildTca>
                            <columns>
                                <uid_local>
                                    <config>
                                        <appearance>
                                            <elementBrowserType>file</elementBrowserType>
                                            <elementBrowserAllowed>gif,jpg,jpeg,tif,tiff,bmp,png</elementBrowserAllowed>
                                        </appearance>
                                    </config>
                                </uid_local>
                            </columns>
                        </overrideChildTca>
                        <filter>
                            <userFunc>TYPO3\CMS\Core\Resource\Filter\FileExtensionFilter->filterInlineChildren</userFunc>
                            <parameters>
                                <allowedFileExtensions></allowedFileExtensions>
                                <disallowedFileExtensions></disallowedFileExtensions>
                            </parameters>
                        </filter>
                        <appearance>
                            <useSortable>1</useSortable>
                            <headerThumbnail>
                                <field>uid_local</field>
                                <width>45</width>
                                <height>45c</height>
                            </headerThumbnail>
							<showPossibleLocalizationRecords>1</showPossibleLocalizationRecords>
							<showRemovedLocalizationRecords>1</showRemovedLocalizationRecords>
							<showSynchronizationLink>1</showSynchronizationLink>
                            <enabledControls>
                                <info>1</info>
                                <new>0</new>
                                <dragdrop>1</dragdrop>
                                <sort>0</sort>
                                <hide>1</hide>
                                <delete>1</delete>
                            </enabledControls>
                        </appearance>
						<behaviour>
							<localizationMode>select</localizationMode>
							<localizeChildrenAtParentLocalization>1</localizeChildrenAtParentLocalization>
						</behaviour>
                    </config>
                </image>
			<bgimgeffect type="array">
				<TCEforms type="array">
					<label>Image effects</label>
					<config type="array">
						<type>selectSingle</type>
						<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">None</numIndex>
								<numIndex index="1">0</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">Lighten</numIndex>
								<numIndex index="1">10</numIndex>
							</numIndex>
							<numIndex index="2" type="array">
								<numIndex index="0">Darken</numIndex>
								<numIndex index="1">20</numIndex>
							</numIndex>
							<numIndex index="3" type="array">
								<numIndex index="0">Blur light</numIndex>
								<numIndex index="1">30</numIndex>
							</numIndex>
							<numIndex index="4" type="array">
								<numIndex index="0">Blur medium</numIndex>
								<numIndex index="1">40</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</bgimgeffect>
			<bgimagewidth type="array">
				<TCEforms type="array">
					<config type="array">
						<type>input</type>
						<size>4</size>
						<eval>trim</eval>
					</config>
					<label>Image width (px)</label>
				</TCEforms>
			</bgimagewidth>
			<bgvideo>
                <label>Background video</label>
                <config>
                    <type>inline</type>
                    <foreign_table>sys_file_reference</foreign_table>
                    <foreign_field>uid_foreign</foreign_field>
                    <foreign_sortby>sorting_foreign</foreign_sortby>
                    <foreign_table_field>tablenames</foreign_table_field>
                    <foreign_match_fields>
                        <fieldname>assets</fieldname>
                    </foreign_match_fields>
                    <foreign_label>uid_local</foreign_label>
                    <foreign_selector>uid_local</foreign_selector>
                    <overrideChildTca>
                        <columns>
                            <uid_local>
                                <config>
                                    <appearance>
                                        <elementBrowserType>file</elementBrowserType>
                                        <elementBrowserAllowed>move,mp4,off,webm</elementBrowserAllowed>
                                    </appearance>
                                </config>
                            </uid_local>
                        </columns>
                    </overrideChildTca>
                    <filter>
                        <userFunc>TYPO3\CMS\Core\Resource\Filter\FileExtensionFilter->filterInlineChildren</userFunc>
                        <parameters>
                            <allowedFileExtensions></allowedFileExtensions>
                            <disallowedFileExtensions></disallowedFileExtensions>
                        </parameters>
                    </filter>
                    <appearance>
                        <useSortable>1</useSortable>
                        <headerThumbnail>
                            <field>uid_local</field>
                            <width>45</width>
                            <height>45c</height>
                        </headerThumbnail>
						<showPossibleLocalizationRecords>1</showPossibleLocalizationRecords>
						<showRemovedLocalizationRecords>1</showRemovedLocalizationRecords>
						<showSynchronizationLink>1</showSynchronizationLink>
                        <enabledControls>
                            <info>1</info>
                            <new>0</new>
                            <dragdrop>1</dragdrop>
                            <sort>0</sort>
                            <hide>1</hide>
                            <delete>1</delete>
                        </enabledControls>
                    </appearance>
					<behaviour>
						<localizationMode>select</localizationMode>
						<localizeChildrenAtParentLocalization>1</localizeChildrenAtParentLocalization>
					</behaviour>
                </config>
            </bgvideo>
			<bgvideosound>
				<TCEforms>
					<label>Video unmute button</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</bgvideosound>
			<bgvideoclearframe>
				<TCEforms>
					<label>Clear frame content button</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</bgvideoclearframe>
			<noloop>
				<TCEforms>
					<label>Disable video loop</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</noloop>
			<bgimgplacement type="array">
				<TCEforms type="array">
					<label>Background alignment</label>
					<config type="array">
						<type>selectSingle</type>
						<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">Center Center</numIndex>
								<numIndex index="1">0</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">Center Left</numIndex>
								<numIndex index="1">1</numIndex>
							</numIndex>
							<numIndex index="2" type="array">
								<numIndex index="0">Center Right</numIndex>
								<numIndex index="1">2</numIndex>
							</numIndex>
							<numIndex index="3" type="array">
								<numIndex index="0">Top Left</numIndex>
								<numIndex index="1">3</numIndex>
							</numIndex>
							<numIndex index="4" type="array">
								<numIndex index="0">Top Center</numIndex>
								<numIndex index="1">4</numIndex>
							</numIndex>
							<numIndex index="5" type="array">
								<numIndex index="0">Top Right</numIndex>
								<numIndex index="1">5</numIndex>
							</numIndex>
							<numIndex index="6" type="array">
								<numIndex index="0">Bottom Left</numIndex>
								<numIndex index="1">6</numIndex>
							</numIndex>
							<numIndex index="7" type="array">
								<numIndex index="0">Bottom Center</numIndex>
								<numIndex index="1">7</numIndex>
							</numIndex>
							<numIndex index="8" type="array">
								<numIndex index="0">Bottom Right</numIndex>
								<numIndex index="1">8</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</bgimgplacement>
			<overlaycolor>
				<TCEforms type="array">
					<config type="array">
						<type>input</type>
						<size>48</size>
						<eval>trim</eval>
						<renderType>colorpicker</renderType>
					</config>
					<label>Background overlay color</label>
				</TCEforms>
			</overlaycolor>
			<overlaycolordarkmode>
				<TCEforms type="array">
					<config type="array">
						<type>input</type>
						<size>48</size>
						<eval>trim</eval>
						<renderType>colorpicker</renderType>
					</config>
					<label>Background overlay color in Dark Mode</label>
				</TCEforms>
			</overlaycolordarkmode>
			<overlayopacity>
				<TCEforms>
					<label>Overlay opacity</label>
					<config>
						<type>input</type>
						<size>1</size>
						<default>5</default>
						<eval>int</eval>
						<range>
							<lower>0</lower>
							<upper>10</upper>
						</range>
						<wizards>
							<slider>
								<type>slider</type>
								<step>1</step>
							</slider>
						</wizards>
					</config>
				</TCEforms>
			</overlayopacity>
			<bgfixed>
				<TCEforms>
					<label>Fixed background</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</bgfixed>
		</el>
	</ROOT>
</T3DataStructure>
